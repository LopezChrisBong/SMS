        <tr>
          <td colspan='2' align='center' class='bordered font-12'>
            Professional/
            <br />Technical
          </td>
          <td colspan='2' align='center' class='bordered font-12'>
            Executive/
            <br />Managerial
          </td>
          <td colspan='2' rowspan='2' align='center' class='bordered font-12'>
            Presidential
            <br />Appointees
          </td>

        </tr>
        <tr>
          <td colspan='2' align='center' class='bordered font-12'>
            (up to SG-24)
          </td>
          <td colspan='2' align='center' class='bordered font-12'>
            (SG-25 and above)
          </td>

        </tr>

        <tr>
          <td align='center' colspan='2' class='bordered font-12' height="20">
           {{_1st_lvl}}
          </td>
          <td colspan='2' align='center' class='bordered font-12' height="20">
            {{_2nd_lvlSG24}}
          </td>
          <td colspan='2' align='center' class='bordered font-12' height="20">
         {{_2nd_lvlSG25}}
          </td>
          </td>
          <td colspan='2' align='center' class='bordered font-12' height="20">
           {{_3rd_lvl}}
          </td>
          </td>
          <td colspan='2' align='center' class='bordered font-12' height="20">
            {{electiveCnt}}
          </td>
          <td colspan='4' align='center' class='bordered font-12' height="20">
            {{total}}
          </td>

        </tr>

        <tr>
          <td align='center' height='10' class='bordered font-12'></td>
          <td align='center' height='10' class='bordered font-12'></td>
