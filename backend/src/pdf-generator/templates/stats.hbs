        {{! 1st level SG-1 to SG 10 }}
        <tr>
          <td align='left' class='bordered font-12' height="20" style='padding-left:5px ;'>
            1st Level
          </td>
          <td align='center' class='bordered font-12' height="20">{{permanent_male_1}}</td>
          <td
            align='center'
            class='bordered font-12' height="20"
          >{{permanent_female_1}}</td>
          <td align='center' class='bordered font-12' height="20">{{temp_male_1}}</td>
          <td align='center' class='bordered font-12' height="20">{{temp_female_1}}</td>
          <td
            align='center'
            class='bordered font-12' height="20"
          >{{coterminous_male_1}}</td>
          <td
            align='center'
            class='bordered font-12' height="20"
