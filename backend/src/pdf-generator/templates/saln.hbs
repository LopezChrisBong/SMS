              <div class='font-12' style="margin-top: 5px;">
                {{#ifCond hasBIFC 1}}
                <input type="checkbox" checked>
                {{else}}
                <input type="checkbox">
                {{/ifCond}}
                
                &nbsp;&nbsp;<i> I/We do not have any business interest and other financial connection.</i></div>
          </center>
