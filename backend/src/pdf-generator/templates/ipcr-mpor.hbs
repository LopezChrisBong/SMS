<html lang='en'>

  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Document</title>
    <style>
      /**body { font-family: 'Liberation Sans'; }*/ .container{ width: 100%;
      margin: 0 auto; } .header { position: fixed; top: 0px; left: 0px; right:
      0px; height: 100px; } .footer { position: fixed; bottom: 0px; left: 0px;
      right: 0px; height: 120px; } body { font-family: 'Arial' } table{ width:
      100%; } table,th, td { page-break-inside: auto; } .strong { font-weight:
      bold; } .bordered { border: 1px solid black; } .font-10 { font-size: 10px;
      } .font-12 { font-size: 12px; } .font-14 {font-size: 14px; } .font-16 {
      font-size: 16px; } .font-18 { font-size:18px; }.font-24 { font-size:24px;
      } .extra-spacing-tr { height: 20px; } .header { height: 80px; } tr
      {page-break-inside: avoid; page-break-after: auto; } thead {
      display:table-header-group; } tfoot { display: table-footer-group; }
      .to_title{ margin-top: 0px;}.to_title_div{ text-align: center; }
      .to_content{page-break-inside: auto; margin-top: 120px; margin-left: 40px;
      margin-right: 40px; } .to_number{margin-right: 100px;} .employee_table{
      }.recommending_approval{ display: flex; justify-content: space-between; }
      .underline{ border-bottom:1px solid black } #eval_table tr td, #eval_table
      tr th{ padding: 8px; } #eval_table{ page-break-after: always; } #f_table
      th,#f_table td,#eval_table th,#eval_table td{ border: 1px solid black; }
      #f_table,#eval_table, #target{ border-collapse: collapse; } 
      #eval_table{ border: 1px solid black; } #target td{ border:
      1px solid black; padding: 2px;} 
      #corefunction{ border-collapse: collapse; } #corefunction td{ border:
      1px solid black; padding: 2px; width: 11.11%;} 
      #header-form{ border-collapse: collapse; width: 15%; height: 10px;} #header-form td{ border:
      1px solid black; } 
      
    </style>
  </head>

  <body>
    {{#each this}}

      <div class='container'>
   <div class='header' style="display: flex;" >
          <img
            src='data:image/png;base64, {{header_img}}'
            width='80%'
            height="140px"
            alt=''
          />
          <div style="width: 5%;">

          </div>
            <table id="header-form" class="font-10">
                <tr>
                    <td>Form No.</td>
                    <td>FM-DNSC-PPE-01</td>
                </tr>
                <tr>
                    <td>Issue Status</td>
                    <td>06</td>
                </tr>
                <tr>
                    <td>Revision No.</td>
                    <td>08</td>
                </tr>
                <tr>
                    <td>Effective Date:</td>
                    <td>06 June 2024</td>
                </tr>
                <tr>
                    <td>Approved by:</td>
                    <td>President</td>
                </tr>
            </table>
        </div>
        <div class='footer'>

          <img
            style='position:fixed; bottom:0; left:0;right:0;'
            src='data:image/png;base64, {{footer_img}}'
            width='100%'
            height="80px"
            alt=''
          />
        </div>
        
          <div style='width: 100%;'>
            <table
              style='margin:0 auto; width: 100%; page-break-inside:auto; table-layout:fixed'
            >
              {{! // Add this css to make main table fixed, child tables will still scroll }}
              <thead>
                <tr>
                  <td colspan='2'>
                    <div style='height: 80px;'></div>
                  </td>
                </tr>
              </thead>
              <tfoot>
                <tr>
                  <td colspan='2'>
                     <div style='height: 80px;'></div>
                  </td>
                </tr>
              </tfoot>
        
              <tbody class=""> 
                <tr>
                    <td colspan="2">
                   <table id="target-header" style="width:100%" class="font-12">
                        <tr>
                           <td class="font-12" ><p style="text-align: center;"> <b>MONTHLY PERFORMANCE OUTPUT REPORT (MPOR)</b></p></td>     
                        </tr>
                        <tr>
                             <td class="font-12" ><p style="text-align: center;"> <b>For the Month of {{month}} {{year}}</b>.</p></td>  
                        </tr>
                    </table>     
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <table id="corefunction" style="width: 100%;" class="font-12">
                            <tr>
                                <td rowspan="3" align="center" style="width: 15%;">MAJOR FINAL OUTPUT</td>
                                <td rowspan="3" align="center" style="width: 15%;">SUCCESS INDICATOR</td>
                                <td colspan="15" align="center" style="width: 75%;">ACTUAL ACCOMPLISHMENT</td>
                                <td rowspan="3" align="center" style="width: 10%;">REMARKS</td>
                            </tr>
                            <tr>
                                <td colspan="5" align="center" style="width: 25%;">QUANTITY (E)</td>
                                <td colspan="5" align="center" style="width: 25%;">QUALITY (Q)</td>
                                <td colspan="5" align="center" style="width: 25%;">TIMELINESS (T)</td>
                            </tr>
                            <tr>
                                {{!-- QUANTITY --}}
                                <td align="center" style="width: 4%;">Week 1</td>
                                <td align="center" style="width: 4%;">Week 2</td>
                                <td align="center" style="width: 4%;">Week 3</td>
                                <td align="center" style="width: 4%;">Week 4</td>
                                <td align="center" style="width: 4%;">Total</td>
                                {{!-- QUALITY --}}
                                <td align="center" style="width: 4%;">Week 1</td>
                                <td align="center" style="width: 4%;">Week 2</td>
                                <td align="center" style="width: 4%;">Week 3</td>
                                <td align="center" style="width: 4%;">Week 4</td>
                                <td align="center" style="width: 4%;">Total</td>
                                {{!-- Timeliness --}}
                                <td align="center" style="width: 4%;">Week 1</td>
                                <td align="center" style="width: 4%;">Week 2</td>
                                <td align="center" style="width: 4%;">Week 3</td>
                                <td align="center" style="width: 4%;">Week 4</td>
                                <td align="center" style="width: 4%;">Total</td>
                            </tr>
                            {{!-- CORE FUNCTION --}}

                            <tr>
                                <td colspan="18">I-CORE FUNCTIONS</td>
                            </tr>
                            {{#cf}}
                            <tr>
                              
                                <td >{{mfo}}</td>
                                <td >{{success_indicator}}</td>
                                {{#ifCond eff_is_rated 1}}
                                <td  align="center" style="width: 4%;">{{mpor.e_w1}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.e_w2}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.e_w3}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.e_w4}}</td>
                                <td  align="center" style="width: 4%;">{{ef_total}}</td>
                                {{else}}
                                <td colspan="5" align="center" style="width: 4%;">Not to be Rated</td>
                                {{/ifCond}}
                              {{#ifCond qly_is_rated 1}}
                                <td  align="center" style="width: 4%;">{{mpor.q_w1}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.q_w2}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.q_w3}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.q_w4}}</td>
                                <td  align="center" style="width: 4%;">{{qly_total}}</td>
                                {{else}}
                                <td colspan="5" align="center" style="width: 4%;">Not to be Rated</td>
                                {{/ifCond}}
                                 {{#ifCond timeliness_is_rated 1}}
                                <td  align="center" style="width: 4%;">{{mpor.t_w1}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.t_w2}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.t_w3}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.t_w4}}</td>
                                <td  align="center" style="width: 4%;">{{tl_total}}</td>
                                {{else}}
                                <td colspan="5" align="center" style="width: 4%;">Not to be Rated</td>
                                {{/ifCond}}
                                <td>{{mpor.remarks}}</td>
                            </tr>
                            {{/cf}}
                                {{!-- STRATEGIC PRIORITY --}}
                            <tr>
                                <td colspan="18">II-STRATEGIC PRIORITY</td>
                              
                            </tr>
                               {{#sp}}
                            <tr>
                              
                                <td >{{mfo}}</td>
                                <td >{{success_indicator}}</td>
                                {{#ifCond eff_is_rated 1}}
                                <td  align="center" style="width: 4%;">{{mpor.e_w1}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.e_w2}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.e_w3}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.e_w4}}</td>
                                <td  align="center" style="width: 4%;">{{ef_total}}</td>
                                {{else}}
                                <td colspan="5" align="center" style="width: 4%;">Not to be Rated</td>
                                {{/ifCond}}
                              {{#ifCond qly_is_rated 1}}
                                <td  align="center" style="width: 4%;">{{mpor.q_w1}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.q_w2}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.q_w3}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.q_w4}}</td>
                                <td  align="center" style="width: 4%;">{{qly_total}}</td>
                                {{else}}
                                <td colspan="5" align="center" style="width: 4%;">Not to be Rated</td>
                                {{/ifCond}}
                                 {{#ifCond timeliness_is_rated 1}}
                                <td  align="center" style="width: 4%;">{{mpor.t_w1}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.t_w2}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.t_w3}}</td>
                                <td  align="center" style="width: 4%;">{{mpor.t_w4}}</td>
                                <td  align="center" style="width: 4%;">{{tl_total}}</td>
                                {{else}}
                                <td colspan="5" align="center" style="width: 4%;">Not to be Rated</td>
                                {{/ifCond}}
                                <td>{{mpor.remarks}}</td>
                            </tr>
                            {{/sp}}

                                {{!-- SUPPORT FUNCTION --}}
                            <tr>
                                <td colspan="18">III-SUPPORT FUNCTION</td>
                              
                            </tr>
                            <tr><td colspan="18"><b>Institutional Obligation</b></td></tr>
                            {{#sf.sf1}}

                            <tr>
                                <td >{{mfo}}</td>
                                <td >{{success_indicator}}</td>
                                {{#ifCond eff_is_rated 1}}
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                {{else}}
                                <td colspan="5" align="center" style="width: 4%;">Not to be Rated</td>
                                {{/ifCond}}
                              {{#ifCond qly_is_rated 1}}
                                 <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                {{else}}
                                <td colspan="5" align="center" style="width: 4%;">Not to be Rated</td>
                                {{/ifCond}}
                                 {{#ifCond timeliness_is_rated 1}}
                               <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                <td  align="center" style="width: 4%;"></td>
                                {{else}}
                                <td colspan="5" align="center" style="width: 4%;">Not to be Rated</td>
                                {{/ifCond}}
                                <td>{{mpor.remarks}}</td>
                            </tr>

                             {{/sf.sf1}}
                            <tr>
                                <td colspan="18" style="padding:10px 0px 10px 10px">Observation and Remarks: <br>(Rater)</td>
                            </tr>
                            <tr>
                                <td colspan="18" style="padding:10px 0px 10px 10px">Observation and Remarks: <br>(Ratee)</td>
                            </tr>
                        
                        </table>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                       <table class="font-12 strong" style="margin: 20px 0px 0px 20px; "> 
                        <tr>
                            <td>
                                    <div>
                                        Above entries are true and correct: <br><br>
                                        <u>{{user.name}}</u> <br>
                                        Name & Signature of Employee <br>
                                        Date Signed:
                                    </div>
                            </td>
                            <td>
                                    <div>
                                      Noted: <br><br>
                                        <u> {{signatory.assesedBy}}</u><br>
                                        Name & Signature of Immediate Supervisor <br>
                                        Date Signed:
                                    </div>
                            </td>
                        </tr>
                       </table> 
                    </td>
                </tr>
              
              </tbody>
            </table>
          </div>

        

      </div>

    {{/each}}
  </body>

</html>