          </tr> --}}

          </tbody>
          <tfoot>
            <tr>
              <td colspan='2'>
                <div style='height: 80px;'></div>

              </td>
            </tr>
          </tfoot>
        </table>

        <table
          id='pcr_table'
          class='collapse'
          style='margin-top:-150px;  width: 100%; page-break-inside:auto'
        >
          {{! // Add this css to make main table fixed, child tables will still scroll }}
          <thead>
            <tr>
              <td colspan='2'>
                <div style='height: 100px;'></div>

              </td>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td
                style='width: 70%;padding:5px'
                align='center'
                class='strong'
              >Performance Category</td>
              <td
                style='width: 30%;padding:5px'
                align='center'
                class='strong'
              >Rating</td>
            </tr>
            {{#pcr}}
              <tr>
                <td style='width: 70%;' class='padding:2px'>
                  <div
                    class='strong'
                    style='padding:0 0 2px 5px; margin-top:-50px'
                  >{{header}}</div>
                  <div style='padding:10px 0 2px 5px'>{{details}}</div>
                </td>
                <td style='width: 30%;' class='padding:5px'>
                  {{#ifCond rating 4}}
                    <div
                      style='display:flex;padding:2px; margin-top:20px'
                    ><input type='checkbox' checked />&nbsp;&nbsp;&nbsp;
                      <label for=''>Exceeds Expectations</label></div>
                    <div style='display:flex;padding:2px'><input
                        type='checkbox'
                      />&nbsp;&nbsp;&nbsp;
                      <label for=''>Meets Expectations</label></div>
                    <div style='display:flex;padding:2px'><input
                        type='checkbox'
                      />&nbsp;&nbsp;&nbsp;
                      <label for=''>Needs Expectations</label></div>
                    <div style='display:flex;padding:2px'><input
                        type='checkbox'
                      />&nbsp;&nbsp;&nbsp;
                      <label for=''>Unacceptable</label></div>
                  {{else}}
                    {{#ifCond rating 3}}
                      <div
                        style='display:flex;padding:2px; margin-top:20px'
                      ><input type='checkbox' />&nbsp;&nbsp;&nbsp;
                        <label for=''>Exceeds Expectations</label></div>
                      <div style='display:flex;padding:2px'><input
                          type='checkbox'
                          checked
                        />&nbsp;&nbsp;&nbsp;
                        <label for=''>Meets Expectations</label></div>
                      <div style='display:flex;padding:2px'><input
                          type='checkbox'
                        />&nbsp;&nbsp;&nbsp;
                        <label for=''>Needs Expectations</label></div>
                      <div style='display:flex;padding:2px'><input
                          type='checkbox'
                        />&nbsp;&nbsp;&nbsp;
                        <label for=''>Unacceptable</label></div>
                    {{else}}
                      {{#ifCond rating 2}}
                        <div
                          style='display:flex;padding:2px; margin-top:20px'
                        ><input type='checkbox' />&nbsp;&nbsp;&nbsp;
                          <label for=''>Exceeds Expectations</label></div>
                        <div style='display:flex;padding:2px'><input
                            type='checkbox'
                          />&nbsp;&nbsp;&nbsp;
                          <label for=''>Meets Expectations</label></div>
                        <div style='display:flex;padding:2px'><input
                            type='checkbox'
                            checked
                          />&nbsp;&nbsp;&nbsp;
                          <label for=''>Needs Expectations</label></div>
                        <div style='display:flex;padding:2px'><input
                            type='checkbox'
                          />&nbsp;&nbsp;&nbsp;
                          <label for=''>Unacceptable</label></div>
                      {{else}}
                        <div
                          style='display:flex;padding:2px; margin-top:20px'
                        ><input type='checkbox' />&nbsp;&nbsp;&nbsp;
                          <label for=''>Exceeds Expectations</label></div>
                        <div style='display:flex;padding:2px'><input
                            type='checkbox'
                          />&nbsp;&nbsp;&nbsp;
                          <label for=''>Meets Expectations</label></div>
                        <div style='display:flex;padding:2px'><input
                            type='checkbox'
                          />&nbsp;&nbsp;&nbsp;
                          <label for=''>Needs Expectations</label></div>
                        <div style='display:flex;padding:2px'><input
                            type='checkbox'
                            checked
                          />&nbsp;&nbsp;&nbsp;
                          <label for=''>Unacceptable</label></div>
                      {{/ifCond}}
                    {{/ifCond}}
                  {{/ifCond}}

                </td>
              </tr>
            {{/pcr}}

          </tbody>
          <tfoot>
            <tr>
              <td colspan='2'>
                <div style='height: 80px;'></div>

              </td>
            </tr>
          </tfoot>
        </table>

