        <table style='margin-top:-150px;   width: 100%; page-break-inside:auto'>
          {{! // Add this css to make main table fixed, child tables will still scroll }}
          <thead>
            <tr>
              <td colspan='2'>
                <div style='height: 100px;'></div>

              </td>
            </tr>
          </thead>
          <tbody>

            <tr><td colspan='2' height='20'><span class='strong'>COMMENT AND
                  SUGGESTIONS<br />(Please assess if there are improvement or
                  none to the performance of the concerned employee/s after
                  attending the seminar or training. You can also include
                  suggestions for the future development of the concerved
                  employee under our unit or office).</span></td></tr>
            <tr>
              <td colspan='2' height='20'></td>
            </tr>
            <tr>
              <td colspan='2'>
                {{#isNotNull comment_sgt}}
                  <div
                    style='border-bottom: 1px solid black;'
                  >{{comment_sgt}}</div>
                {{else}}
                  <div style='border-bottom: 1px solid black;'></div>
                  <div
                    style='margin-top:20px;border-bottom: 1px solid black;'
                  ></div>
                  <div
                    style='margin-top:20px;border-bottom: 1px solid black;'
                  ></div><div
                    style='margin-top:20px;border-bottom: 1px solid black;'
                  ></div>
                {{/isNotNull}}

              </td>
            </tr>

            <tr>
              <td colspan='2' height='80'></td>
            </tr>
            <tr>
              <td style='width: 50%;' height='20'>
                <div style='border-bottom: 1px solid black;'>{{toUpperCase
                    rater_name
                  }}</div>
                <div class='font-14'>Signature over Printed Name of
                  Supervisor/Rater</div>
              </td>
              <td style='width: 50%;'></td>
            </tr>
            <tr>
              <td colspan='2' height='10'></td>
            </tr>
            <tr>
              <td style='width: 100%;display:flex' height='20'>

                <div class='font-14' style='width:10%'>Date:
                </div>
                <div
                  class='font-14'
                  style='width:90%;border-bottom: 1px solid black;'
                >{{formatAlphanumericDate date_rated}}</div>
              </td>
              <td style='width: 50%;'></td>
            </tr>

          </tbody>
          <tfoot>
            <tr>
              <td colspan='2'>
                <div style='height: 80px;'></div>

              </td>
            </tr>
          </tfoot>
        </table>
      </div>
      {{!--  {{/TO}} --}}
    {{/ea}}
  </body>

</html>